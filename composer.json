{
    "name": "mrobles/laravel-sendgrid-webhooks",
    "description": "Laravel package for receiving and storing event webhooks from Sendgrid",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kit Brennan",
            "email": "kitbrennan90@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "sendgrid/sendgrid": "~7"
    },
    "autoload": {
        "psr-4": {
            "LaravelSendgridWebhooks\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSendgridWebhooks\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "~5.8.3",
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^7.5"
    }
}
